{
	"name": "petstore/web-app",
	"require": {
		"php": "^8.3",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"contributte/api-router": "^6.0.0",
		"netresearch/jsonmapper": "^5.0.0",
		"symfony/serializer": "^7.1.6",
		"symfony/property-access": "^7.1.6",
		"phpdocumentor/reflection-docblock": "^5.5.1"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.12.8",
		"phpunit/phpunit": "^11.4.3",
		"dg/bypass-finals": "^1.8.0"
	},
	"autoload": {
		"psr-4": {
			"PetStore\\": "app"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PetStore\\Tests\\": "tests"
		}
	},
	"minimum-stability": "stable",
	"scripts":
	{
		"phpstan": "vendor/bin/phpstan analyse -c phpstan.neon app/ tests/",
		"phpunit": "vendor/bin/phpunit tests --bootstrap tests/bootstrap.php"
	},
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
