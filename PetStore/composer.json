{
	"name": "petstore/web-app",
	"require": {
		"php": "^8.3",
		"nette/application": "^3.2.3",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"symfony/thanks": "^1",
		"phpstan/phpstan": "^1.12.8"
	},
	"autoload": {
		"psr-4": {
			"PetStore\\": "app"
		}
	},
	"minimum-stability": "stable",
	"scripts":
	{
		"phpstan": "vendor/bin/phpstan analyse -c phpstan.neon app/ tests/"
	},
	"config": {
		"allow-plugins": {
			"symfony/thanks": true
		}
	}
}
