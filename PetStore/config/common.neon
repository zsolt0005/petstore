parameters:

services:
    - PetStore\Router\RouterFactory::createRouter

application:
	errorPresenter:
		4xx: Error:Error4xx
		5xx: Error:Error5xx
	mapping: PetStore\*\**Presenter

latte:
	strictTypes: yes
	strictParsing: yes

di:
	export:
		parameters: no
		tags: no

search:
	-	in: %appDir%
		classes:
			- *Factory
			- *Facade